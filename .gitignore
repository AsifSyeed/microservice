# Ignore Maven target directory
/target/

# IntelliJ IDEA specific files
.idea/
/*.iml
*.iws
*.ipr

# IntelliJ IDEA project files and settings
.idea/**/workspace.xml
.idea/**/tasks.xml
.idea/**/usage.statistics.xml
.idea/**/dictionaries
.idea/**/shelf

# Ignore all IntelliJ project files except the code style settings
# These can be useful for maintaining consistent code styles among team members
!/.idea/codeStyles
.idea/codeStyles/*

# Maven specific files
.mvn/
!/.mvn/wrapper/maven-wrapper.jar
pom.xml.tag
pom.xml.releaseBackup
pom.xml.versionsBackup
pom.xml.next
release.properties

# Eclipse or other IDE-specific files (just in case)
.classpath
.project
.settings/
.loadpath

# OS-specific files
.DS_Store
Thumbs.db
*.swp

# Logs and temp files
*.log
logs/
*.tmp

# Bytecode and compiled files
*.class

# Ignore Spring Boot devtools restart directory
.spring-boot-devtools/

# Ignore temporary files from development
.env
*.env

# Ignore build artifacts
/out/
dist/
build/

# Exclude JAR/WAR files generated by the build
*.jar
*.war
*.ear

# Lombok generated files
.lombok/

# Ignore test reports and code coverage files
/test-output/
/jacoco.exec
/build/

# Ignore specific IDE plugin files
/.idea/**/vcs.xml
